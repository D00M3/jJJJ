 pipeline {
    agent any
    stages {
        stage('Pipeline Stages'){
            steps {
                sh "ls"
} }
        stage(‘second stage’){
            steps {
sh “pwd” }
} }
        stage(‘third stage’){
            steps {
sh “touch docker.txt” }
} }
         stage(‘fouth stage’){
            steps {
sh “mv docker.txt mydir” }
} }